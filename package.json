{
  "name": "seras-kitchen-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Sera's Kitchen Frontend - React TypeScript Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test-backend": "node test-backend-connection.js",
    "dev:local": "VITE_API_URL=http://localhost:5000 vite",
    "dev:prod": "VITE_API_URL=https://hamilton47.pythonanywhere.com vite",
    "deploy:netlify": "npm run build && echo 'Build complete! Upload dist folder to Netlify'",
    "deploy:vercel": "npm run build && npx vercel --prod",
    "deploy:vercel:dev": "npm run build && npx vercel",
    "vercel-build": "npm run build",
    "start": "vite preview --port $PORT --host 0.0.0.0",
    "clean": "rm -rf dist node_modules package-lock.json",
    "fresh-install": "npm run clean && npm install",
    "postbuild": "echo 'Build completed successfully for production deployment'"
  },
  "dependencies": {
    "caniuse-lite": "^1.0.30001732",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.15",
    "terser": "^5.36.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
